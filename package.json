{
  "name": "vue-todo-app",
  "version": "1.0.1",
  "description": "A simple and responsive Vue.js To-Do application for managing tasks efficiently.",
  "homepage": "https://reza-nzri.github.io/vue-todo-app/",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node getVersion.js && vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "run-p type-check \"build-only {@}\" -- && vue-cli-service build",
    "prebuild": "node getVersion.js",
    "prestart": "node getVersion.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "start": "vue-cli-service serve",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@types/vue": "^2.0.0",
    "@vueuse/core": "^10.11.0",
    "pinia": "^2.1.7",
    "prettier": "^3.3.2",
    "vite-plugin-tsconfig-paths": "^1.4.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/compiler-sfc": "^3.4.30",
    "@vue/devtools": "^6.6.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "sass": "^1.77.8",
    "sass-loader": "^10.5.2",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
}